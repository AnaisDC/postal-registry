{
  "ConnectionStrings": {
    "Events": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.PostalRegistry;Trusted_Connection=True;",
    "ExtractProjections": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.PostalRegistry;Trusted_Connection=True;",
    "ExtractProjectionsAdmin": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.PostalRegistry;Trusted_Connection=True;"
  },

  "DataDog": {
    "Enabled": false,
    "Debug": false
  },

  "Cors": [
    "http://localhost:3000",
    "http://localhost:5000"
  ],

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "PostalRegistry": "Debug",
      "Be.Vlaanderen.Basisregisters": "Debug",
      "Be.Vlaanderen.Basisregisters.DataDog.Tracing": "Debug",
      "Be.Vlaanderen.Basisregisters.AspNetCore.Mvc.Logging": "Information",
      "Marvin": "Debug",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "PostalRegistry": "Verbose",
        "Be.Vlaanderen.Basisregisters": "Verbose",
        "Be.Vlaanderen.Basisregisters.DataDog.Tracing": "Verbose",
        "Be.Vlaanderen.Basisregisters.AspNetCore.Mvc.Logging": "Error",
        "Marvin": "Verbose",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Properties": {
      "Application": "PostalRegistry - Extract API",
      "ContainerId": "REPLACE_CONTAINERID"
    }
  }
}
